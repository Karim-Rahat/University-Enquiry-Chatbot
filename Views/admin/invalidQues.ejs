<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../template/style') %>
        <style>
            ol li {
                list-style: square;
                list-style-position: inside;
            }
        </style>
</head>

<body >


    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
  
    <div id="main-wrapper">

        <%- include('../template/sideNav') %>

            <div class="content-body">
       
                <div class="container-fluid">
                    <div class="form-head align-items-center d-flex mb-sm-4 mb-3">
                        <div class="mr-auto">
                            <h2 class="text-black font-w600">University Enquiry Chatbot Dashboard</h2>
                            <p class="mb-0"></p>
                        </div>

                    </div>

                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <h4 class="card-title"> Invalid Question Answer List</h4>
                                </div>
                                <div class="card-body">
                                    <div class="table-responsive ">
                                        <table class="table table-striped text-black table-bordered">
                                            <thead>
                                                <tr>

                                                    <th style="font-size: 14px;width: 100px; font-weight: bolder;">Chat ID</th>
                                                    <th style="font-size: 14px;width: 100px;  font-weight: bolder;">User ID</th>
                                                    <th style="font-size: 14px;width: 270px;  font-weight: bolder;">Question</th>
                                                    <th style="font-size: 14px;width: 270px;  font-weight: bolder;">Answer</th>

                                                
                                                    <th style="font-size: 14px; font-weight: bolder;">Created Date</th>


                                                 
                                                </tr>
                                            </thead>
                                            <tbody id="queryList" style="font-size: 14px;">
                                                <% data.map(item=>{ %>


                                                    <tr>
                                                        <td><%=item.chat_id %> </td>
                                                        <td><%=item.userId %> </td>
                                                        <td><%=item.question%> </td>
                                                        <td><%=item.answer.replace("<br>","") %> </td>
                                                      
                                                        <td><%=item.timestamp %> </td>
                                             
                                                    </tr>
                                                    <% }) %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    </div>
 
    <div class="footer">

    </div>
  
    </div>
   
</body>


<script src="/./vendor/global/global.min.js"></script>
<script src="/./vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
<script src="/./js/custom.min.js"></script>
<script src="/./js/deznav-init.js"></script>

<script src="/./vendor/datatables/js/jquery.dataTables.min.js"></script>
<script src="/./js/plugins-init/datatables.init.js"></script>

</html>